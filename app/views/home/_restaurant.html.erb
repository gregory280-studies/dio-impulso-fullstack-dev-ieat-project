<p>Seus produtos, <%= current_user.restaurant.name %></p>
<table>
  <thead>
    <tr>
      <th>nome</th>
      <th>valor</th>
    </tr>
  </thead>
  <tbody>
    <% current_user.restaurant.products.each do |product| %>
    <tr>
      <th><%= product.name %></th>
      <th><%= product.value %></th>
    </tr>
    <% end %>
  </tbody>
</table>